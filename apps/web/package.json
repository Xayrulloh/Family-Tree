{
  "name": "@family-tree/web",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@family-tree/shared": "workspace:*"
  },
  "nx": {
    "targets": {
      "lint": {
        "executor": "nx:run-commands",
        "options": {
          "command": "biome check",
          "cwd": "apps/web"
        }
      },
      "format": {
        "executor": "nx:run-commands",
        "options": {
          "command": "biome check --write --unsafe",
          "cwd": "apps/web"
        }
      },
      "scan": {
        "executor": "nx:run-commands",
        "options": {
          "command": "sonar",
          "cwd": "apps/web"
        }
      }
    }
  },
  "dependencies": {
    "@sentry/react": "^10.32.1",
    "save-svg-as-png": "^1.4.17"
  }
}
